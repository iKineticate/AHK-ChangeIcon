; Base64转PNG的函数
Base64PNGToHICON(Base64PNG, height := 64) {
    size := StrLen(RTrim(Base64PNG, '='))
    if DllCall('Crypt32\CryptStringToBinary'
        , 'Str'  , Base64PNG
        , 'UInt' , StrLen(Base64PNG)
        , 'UInt' , 0x00000001
        , 'Ptr'  , buf := Buffer(size)
        , 'UIntP', &size
        , 'Ptr'  , 0
        , 'Ptr'  , 0)
    return DllCall('CreateIconFromResourceEx'
        , 'Ptr' , buf
        , 'UInt', size
        , 'UInt', true
        , 'UInt', 0x30000
        , 'Int' , height
        , 'Int' , height
        , 'UInt', 0)
    return 0
}


